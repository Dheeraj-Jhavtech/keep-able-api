{
    "name": "express-ts-starter",
    "version": "0.0.1",
    "description": "Express.ts Server starter template",
    "main": "index.js",
    "scripts": {
        "start:dev:unix": "NODE_ENV=local nodemon -r dotenv/config src/index.ts",
        "start:dev:win": "set NODE_ENV=local && nodemon -r dotenv/config src/index.ts",
        "start:stage:unix": "NODE_ENV=staging node dist/index.js",
        "start:stage:win": "set NODE_ENV=staging && node dist/index.js",
        "start:prod:unix": "NODE_ENV=production node dist/index.js",
        "start:prod:win": "set NODE_ENV=production && node dist/index.js",
        "start:prod:local:unix": "NODE_ENV=production.local node dist/index.js",
        "start:prod:local:win": "set NODE_ENV=production.local && node dist/index.js",
        "build": "tsc -p .",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "create-env": "node create-env.js"
    },
    "keywords": [
        "express",
        "typescript",
        "starter",
        "template",
        "starter-template",
        "express.ts",
        "expressjs",
        "express.js",
        "mongodb",
        "mongoose",
        "socket.io"
    ],
    "author": "Deo Subarno",
    "license": "MIT",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.4",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.5",
        "@types/node": "^20.5.9",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "cli-color": "^2.0.3",
        "eslint": "^8.48.0",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@typegoose/typegoose": "^11.5.0",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.11.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.5.0",
        "morgan": "^1.10.0",
        "socket.io": "^4.7.2"
    }
}
